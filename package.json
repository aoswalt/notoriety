  },
  "homepage": "https://github.com/aoswalt/notoriety#readme",
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.9",
    "bs-platform": "^7.1.0"
  },
  "dependencies": {
    "bs-log": "^1.1.0",
    "bs-yaml": "^1.0.2"
  }
